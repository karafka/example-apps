<h3>Visitor recent visits</h3>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Request id</th>
      <th>Visited at</th>
      <th>Page path</th>
    </tr>
  </thead>
  <tbody>
    <% @visits.each do |visit| %>
      <tr>
        <td><%= visit.id %></td>
        <td><%= visit.visited_at %></td>
        <td><%= visit.page_path %></td>
      </tr>
    <% end %>
  </tbody>
</table>
