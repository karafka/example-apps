<h3>Newly created visitors</h3>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Id</th>
      <th>Created at</th>
    </tr>
  </thead>
  <tbody>
    <% @visitors.each do |visitor| %>
      <tr>
        <td><%= link_to visitor.id, visitor %></td>
        <td><%= visitor.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
